pipeline {
  agent any
  stages {
    stage('Build'){
      steps {
        echo "Build step"
        sleep 10
      }
    }  
    stage('Test'){
      steps {
        echo "Test step"
        sleep 10
      }
    }  
  stage('deploy'){
      steps {
        echo "Deploy step"
        sleep 10
      }
    }  
  stage('docker'){
      steps {
        echo "docker step"
        sleep 10
      }
    }  
}
    
